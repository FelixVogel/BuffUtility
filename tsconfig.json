{
    "compilerOptions": {
        "module": "none",
        "target": "es5",
        "inlineSourceMap": true,
        "inlineSources": true,
        "lib": [
            "ES2017.Object",
            "ES2015",
            "dom"
        ],
        "resolveJsonModule": true
    },
    "exclude": [
        "node_modules"
    ],
    "files": [
        "sources/.GlobalConstants.ts"
    ],
    "include": [
        "sources/@typings/*.ts",
        "sources/*.ts",
        "sources/adjustments/*.ts",
        "sources/csgostash/*.ts"
    ]
}