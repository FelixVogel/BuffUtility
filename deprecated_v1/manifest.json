{
	"name": "Buff Utility",
	"version": "1.0.6",
	"description": "Adds cosmetic functionality to buff.163.com. Extends csgostash.com to show buff pricing.",
	"manifest_version": 2,
	"content_scripts": [
		{
			"js": ["out/buff_utility.js"],
			"matches": ["https://buff.163.com/*"]
		},
		{
			"js": ["out/csgostash_utility.js"],
			"matches": ["https://csgostash.com/*"]
		}
	],
	"permissions": [],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}